{
    "mcpServers": {
        "firebase": {
            "command": "npx",
            "args": [
                "-y",
                "firebase-tools@latest",
                "experimental:mcp"
            ]
        }
    },
    "hosting": {
        "rewrites": [
            {
                "source": "/api/**",
                "destination": "https://cdn.real.discount/**"
            }
        ]
    }
}
